<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="icon" href="/images/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/css/style.css">
    <script src="https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js"></script>
</head>
<body>
    <div id="particles-js"></div>

    <nav>
        <ul>
            <li>
                <a href="/">
                    <img src="/images/logo2.png" alt="Logo" style="height: 40px; vertical-align: middle;">
                </a>
            </li>
            <li><a href="/">Home</a></li>
            <li><a href="/about">About</a></li>
            <li><a href="/projects">Projects</a></li>
            <li><a href="/credentials">Credentials</a></li>
        </ul>
    </nav>

    <div class="credentials-container">
        <h1>My Credentials</h1>
        
        <div class="credentials-list">
            <% credentials.forEach(credential => { %>
                <div class="credential-item">
                    <img src="<%= credential.image %>" alt="<%= credential.name %>" download>
                    <h2><%= credential.name %></h2>
                    <p><%= credential.details %></p>
                </div>
            <% }) %>
        </div>
    </div>

    <!-- Footer -->
    <footer>
        <div class="footer-container">
            <p>&copy; 2025 Rojhon L. Abalos. All rights reserved.</p>
            <div class="social-links">
                <!-- Social links as in the previous template -->
            </div>
        </div>
    </footer>

    <!-- Scripts -->
    <script src="/js/particles-config.js"></script>
    <script src="/js/app.js"></script>
</body>
</html>